                "multiline": true,
                "value": "from axiestudio.custom import Component\r\nfrom axiestudio.inputs import MessageTextInput\r\nfrom axiestudio.template import Output\r\nfrom axiestudio.schema import Data\r\nfrom newspaper import Article\r\n\r\nclass ExtractArticleTextComponent(Component):\r\n    display_name = \"Extract Article Text\"\r\n    description = \"Extrai o texto completo de um artigo a partir de uma URL.\"\r\n    icon = \"newspaper\"\r\n\r\n    inputs = [\r\n        MessageTextInput(\r\n            name=\"article_url\",\r\n            display_name=\"Article URL\",\r\n            info=\"URL da página da notícia.\",\r\n        ),\r\n    ]\r\n\r\n    outputs = [\r\n        Output(display_name=\"Article Text\", name=\"article_text\", method=\"extract_text\"),\r\n    ]\r\n\r\n    def extract_text(self) -> Data:\r\n        article_url = self.article_url\r\n\r\n        # Cria um objeto Artigo e faz o download do conteúdo\r\n        article = Article(article_url)\r\n        article.download()\r\n        article.parse()\r\n\r\n        # Obtém o texto do artigo\r\n        article_text = article.text\r\n\r\n        # Retorna o texto do artigo\r\n        return Data(data={\"article_text\": article_text})\r\n",
