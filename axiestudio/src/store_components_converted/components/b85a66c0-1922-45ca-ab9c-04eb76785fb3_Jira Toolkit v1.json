                "list": false,
                "show": true,
                "multiline": true,
                "value": "from typing import Union, Optional\nfrom langchain_community.utilities.jira import JiraAPIWrapper\nfrom langchain_community.agent_toolkits.jira.toolkit import JiraToolkit\nfrom langchain.agents import AgentType, initialize_agent\nfrom axiestudio.base.langchain_utilities.model import LCToolComponent\nfrom axiestudio.inputs import SecretStrInput, MessageTextInput, DictInput, BoolInput\nfrom axiestudio.schema import Data\nfrom axiestudio.field_typing import Tool, BaseTool\nfrom langchain.tools import StructuredTool\nfrom pydantic import BaseModel, Field\nfrom axiestudio.io import Output, BoolInput\n\n\nclass JiraToolComponent(LCToolComponent):\n    display_name: str = \"Jira Tool v.1\"\n    description: str = \"Tools for interacting with Jira\"\n    name = \"JiraTool\"\n    beta = True\n    documentation: str = \"https://python.langchain.com/v0.2/docs/integrations/tools/jira/\"\n    icon = \"langchain_utilities\"\n    inputs = [\n        MessageTextInput(name=\"jira_username\", display_name=\"Atlassian Account's Email\", required=True),\n        SecretStrInput(name=\"jira_api_token\", display_name=\"Atlassian Account's API Token\", required=True),\n        MessageTextInput(name=\"jira_instance_url\", display_name=\"Jira Instance URL\", required=True),\n        MessageTextInput(name=\"jira_cloud\", display_name=\"Jira Cloud\", required=True, value=\"True\", advanced=True),\n    ]\n    outputs = [\n        Output(name=\"api_run_model\", display_name=\"Data\", method=\"run_model\"),\n        Output(name=\"api_build_tool\", display_name=\"Tool\", method=\"build_tool\"),\n    ]\n\n    def JiraAPIWrapper(self) -> JiraAPIWrapper:\n        jira_username = self.inputs[0].value\n        jira_api_token = self.inputs[1].value\n        jira_instance_url = self.inputs[2].value\n        jira_cloud = bool(self.inputs[3].value)\n        \n        return JiraAPIWrapper(\n            jira_api_token=jira_api_token,\n            jira_instance_url=jira_instance_url,\n            jira_cloud=jira_cloud,\n            jira_username=jira_username\n        )\n    \n    def build_tool(self) -> Tool:\n        jira_username = self.inputs[0].value\n        jira_api_token = self.inputs[1].value\n        jira_instance_url = self.inputs[2].value\n        jira_cloud = self.inputs[3].value\n\n        jira = JiraAPIWrapper(jira_api_token=jira_api_token, jira_instance_url=jira_instance_url, jira_cloud=jira_cloud, jira_username=jira_username)\n        toolkit = JiraToolkit.from_jira_api_wrapper(jira)\n        \n        for tool in toolkit.tools:\n            tool.name = tool.name.replace(\" \", \"_\")\n        \n        return toolkit.tools",
