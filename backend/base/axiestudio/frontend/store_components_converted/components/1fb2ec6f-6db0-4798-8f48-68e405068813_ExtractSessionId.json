                "value": "from axiestudio import CustomComponent\nfrom typing import Union, Dict\nfrom json import loads\nimport re\n\n\nclass ExtractSessionId(CustomComponent):\n    display_name = \"ExtractSessionId\"\n    description = \"Extract chat session_id from the given input.\"\n    field_order = [\"data\"]\n    icon = \"Streamlit\"\n\n    def build_config(self) -> dict:\n        return {\n            \"data\":{\n                \"display_name\": \"Input\",\n                \"required\": True\n            }\n        }\n\n    def build(self, data: str) -> str:\n        data = loads(data)\n        regex = r\"[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}\"\n        if isinstance(data, dict):\n            if \"session_id\" in data:\n                print(data, flush=True)\n                if re.match(regex, data[\"session_id\"]):\n                    return data[\"session_id\"]\n                else:\n                    raise ValueError(\"It is invalid as uuid\")\n            raise Exception(\"It has no session to be extracted\")\n        if re.match(regex, data):\n            return data\n        raise ValueError(\"It is invalid as uuid\")\n",
