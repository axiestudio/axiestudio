# Railway Environment Variables Template
# Copy these to your Railway project environment variables

# Axie Studio Configuration
AXIESTUDIO_AUTO_LOGIN=false
AXIESTUDIO_AUTO_SAVING=true
AXIESTUDIO_CACHE_TYPE=memory
AXIESTUDIO_DATABASE_MAX_OVERFLOW=20
AXIESTUDIO_DATABASE_POOL_RECYCLE=3600
AXIESTUDIO_DATABASE_POOL_SIZE=10
AXIESTUDIO_DATABASE_POOL_TIMEOUT=30
AXIESTUDIO_DEBUG=false
AXIESTUDIO_FALLBACK_TO_ENV_VAR=true
AXIESTUDIO_HOST=0.0.0.0
AXIESTUDIO_JWT_SECRET=your-production-jwt-secret-here
AXIESTUDIO_LOG_LEVEL=INFO
AXIESTUDIO_NEW_USER_IS_ACTIVE=true
AXIESTUDIO_OPEN_BROWSER=false
AXIESTUDIO_PORT=7860
AXIESTUDIO_SAVE_DB_IN_CONFIG_DIR=false
AXIESTUDIO_SECRET_KEY=your-production-secret-key-here
AXIESTUDIO_STORE=false
AXIESTUDIO_STORE_ENVIRONMENT_VARIABLES=true
AXIESTUDIO_SUPERUSER=your-admin-email@domain.com
AXIESTUDIO_SUPERUSER_PASSWORD=your-secure-admin-password
AXIESTUDIO_WORKERS=1

# Database Configuration (Use Railway PostgreSQL)
AXIESTUDIO_DATABASE_URL=postgresql://username:password@host:port/database

# Stripe Configuration (Replace with your actual Stripe keys)
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PRICE_ID=price_1RxD0RBacFXEnBmNI6T0IkOd

# Railway Deployment Configuration
FRONTEND_URL=https://your-railway-app.railway.app
RAILWAY_ENVIRONMENT=production

# Optional: Disable tracking
DO_NOT_TRACK=1
