                "multiline": true,
                "value": "from typing import Union\r\nfrom langchain_core.tools import Tool\r\nfrom axiestudio.base.langchain_utilities.model import LCToolComponent\r\nfrom axiestudio.inputs import MultilineInput\r\nfrom axiestudio.schema import Data\r\n\r\nclass TextProcessingToolComponent(LCToolComponent):\r\n    display_name = \"Documentação do LangFlow\"\r\n    description = \"Contém a Documentação Sobre O LangFlow.\"\r\n    name = \"LangFlowDocsProcessingTool\"\r\n\r\n    inputs = [\r\n        MultilineInput(\r\n            name=\"input_text\",\r\n            display_name=\"Input Text\",\r\n            info=\"Enter the text you want to process.\",\r\n            value=\"\",\r\n        ),\r\n        MultilineInput(\r\n            name=\"result_text\",\r\n            display_name=\"Result Text\",\r\n            info=\"Text processed by the query.\",\r\n        ),\r\n    ]\r\n\r\n    #def run_model(self) -> Union[Data, list[Data]]:\r\n    def run_model(self, input_text: Optional[str] = None) -> Union[Data, List[Data]]:\r\n        if input_text is None:\r\n            input_text = self.input_text  # Use self.input_text if no input_text is provided\r\n\r\n        if not input_text:\r\n            raise ValueError(\"No input_text provided and self.input_text is empty.\")\r\n\r\n        # Create the output data\r\n        data = Data(answer=self.result_text)\r\n\r\n        # Update the status\r\n        self.status = data\r\n        \r\n        print(\"data\", data)\r\n\r\n        return data\r\n\r\n    def build_tool(self) -> Tool:\r\n        # Create a tool that uses the run_model function\r\n        return Tool(\r\n            name=\"axiestudio_text_processing_tool\",\r\n            description=\"A tool that processes axiestudio documentation.\",\r\n            func=self.run_model,\r\n        )\r\n",
