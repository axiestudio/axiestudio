                "multiline": true,
                "value": "# from axiestudio.field_typing import Data\r\nfrom axiestudio.custom import Component\r\nfrom axiestudio.io import MessageTextInput, Output\r\nfrom axiestudio.schema import Data\r\nfrom openai import OpenAI\r\n\r\n\r\nclass WhisperComponent(Component):\r\n    display_name = \"Whisperer\"\r\n    description = \"Audio to text with Whisper OpenAI model\"\r\n    icon = \"file-audio\"\r\n    name = \"Whisperer\"\r\n    \r\n    inputs = [\r\n        SecretStrInput(\r\n            name=\"OpenAIKey\",\r\n            display_name=\"OpenAIKey\",\r\n            info=\"\"\r\n        ),\r\n        FileInput(\r\n            name=\"AudioFileName\",\r\n            display_name=\"AudioFileName\",\r\n            file_types=[\"mp3\", \"mp4\", \"m4a\"],\r\n            info=f\"Supported file types: mp3, mp4, m4a\",\r\n        ),\r\n    ]\r\n\r\n    outputs = [\r\n        Output(display_name=\"Output\", name=\"output\", method=\"build_output\"),\r\n    ]\r\n\r\n    def build_output(self) -> str:\r\n        \r\n        client = OpenAI(api_key=self.OpenAIKey)\r\n        \r\n        audio_file= open(self.AudioFileName, \"rb\")\r\n        transcript = client.audio.transcriptions.create(\r\n          model=\"whisper-1\", \r\n          file=audio_file,\r\n          response_format=\"text\"\r\n        )\r\n        self.status = transcript\r\n        return transcript",
