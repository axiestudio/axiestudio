                "list": false,
                "show": true,
                "multiline": true,
                "value": "from typing import List\r\nimport tweepy\r\nfrom sentence_transformers import SentenceTransformer\r\nfrom axiestudio import CustomComponent\r\nfrom axiestudio.field_typing import Document\r\n\r\n\r\nclass TwitterCrawlerComponent(CustomComponent):\r\n    display_name = \"Twitter Crawler\"\r\n    description = \"根据输入的网红姓名爬取推特上的所有评论和动态，并生成Document对象。\"\r\n    documentation = \"https://developer.twitter.com/en/docs\"\r\n\r\n    def build_config(self):\r\n        return {\r\n            \"celebrity_name\": {\"display_name\": \"网红姓名\", \"type\": \"string\"},\r\n            \"api_key\": {\"display_name\": \"API Key\", \"type\": \"string\"},\r\n            \"api_secret_key\": {\"display_name\": \"API Secret Key\", \"type\": \"string\"},\r\n            \"access_token\": {\"display_name\": \"Access Token\", \"type\": \"string\"},\r\n            \"access_token_secret\": {\"display_name\": \"Access Token Secret\", \"type\": \"string\"},\r\n        }\r\n\r\n    def build(\r\n        self,\r\n        celebrity_name: str,\r\n        api_key: str,\r\n        api_secret_key: str,\r\n        access_token: str,\r\n        access_token_secret: str,\r\n    ) -> List[Document]:\r\n        # 认证并初始化 Tweepy API\r\n        auth = tweepy.OAuth1UserHandler(\r\n            api_key, api_secret_key, access_token, access_token_secret\r\n        )\r\n        api = tweepy.API(auth)\r\n\r\n        # 爬取推特数据\r\n        tweets = []\r\n        for tweet in tweepy.Cursor(api.user_timeline, screen_name=celebrity_name, tweet_mode=\"extended\").items():\r\n            tweets.append(tweet.full_text)\r\n\r\n        # 将推特文本转换为 Document 对象\r\n        documents = [Document(text=tweet) for tweet in tweets]\r\n\r\n        return documents",
