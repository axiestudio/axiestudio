                "multiline": true,
                "value": "# from axiestudio.field_typing import Data\nfrom axiestudio.schema import Record\nfrom axiestudio.interface.custom.custom_component import CustomComponent\nfrom datasets import DatasetDict\n\n\nclass NameExtractor(CustomComponent):\n    display_name = \"Name Extractor\"\n    description = \"Extract the name out of the text\"\n    icon = \"custom_components\"\n\n    def build(self, prompt: str, dataset: DatasetDict, model_component: ModelAdapter) -> DatasetDict:\n        df = dataset['train'].to_pandas().head(15)\n        responses = []\n        \n        for text in df['text']:\n            chat_completion = model_component.create_completion(\n                messages=[{\"role\": \"user\", \"content\": prompt + text}]\n                )\n            responses.append(chat_completion.choices[0].message.content)\n        \n        df['answer'] = responses\n        new_dataset = Dataset.from_pandas(df)\n        \n        return new_dataset\n",
