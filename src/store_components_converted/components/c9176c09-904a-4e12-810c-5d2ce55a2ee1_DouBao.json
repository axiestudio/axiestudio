                "list": false,
                "show": true,
                "multiline": true,
                "value": "from volcenginesdkarkruntime import Ark\nfrom axiestudio.custom import Component\nfrom axiestudio.io import StrInput, DropdownInput, MessageTextInput, Output\nfrom axiestudio.schema.message import Message\n\nMODEL_MAPPING = {\n    \"Doubao-pro-32k-241215\": \"ep-20250117114432-pfqvq\",\n    \"Doubao-pro-32k-character-241215\": \"ep-20241206114453-lxnmz\",\n}\n\nclass DouBaoComponent(Component):\n    display_name = \"DouBao\"\n    description = \"Integrate DouBao models for text processing.\"\n    icon = \"message-square\"\n    # 定义输入参数\n    inputs = [\n        MessageTextInput(\n            name=\"user_input\",\n            display_name=\"User Input\",\n            info=\"Enter your message here.\",\n        ),\n        StrInput(\n            name=\"api_key\",\n            display_name=\"API Key\",\n            info=\"Your DouBao API key.\",\n        ),\n        DropdownInput(\n            name=\"model\",\n            display_name=\"Model\",\n            options=[\"Doubao-pro-32k-241215\", \"Doubao-pro-32k-character-241215\"],\n            value=\"Doubao-pro-32k-241215\",\n            info=\"Select the DouBao model.\",\n        ),\n    ]\n    # 定义输出\n    outputs = [\n        Output(\n            name=\"response\",\n            display_name=\"Response\",\n            method=\"generate_response\",\n        ),\n    ]\n    def generate_response(self) -> Message:\n        client = Ark(api_key=self.api_key)\n        try:\n            selected_model = MODEL_MAPPING.get(self.model, self.model)\n            response = client.chat.completions.create(\n                model=selected_model,\n                messages=[{\"role\": \"user\", \"content\": self.user_input}]\n            )\n            reply = response.choices[0].message.content\n            return Message(text=reply)\n        except Exception as e:\n            return Message(text=f\"Error: {str(e)}\")\n\n",
