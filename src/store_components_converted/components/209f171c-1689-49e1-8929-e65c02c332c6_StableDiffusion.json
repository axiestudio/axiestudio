                "list": false,
                "show": true,
                "multiline": true,
                "value": "from axiestudio.custom import Component\nfrom axiestudio.io import MessageTextInput, Output, DropdownInput, SecretStrInput\n\nimport requests\nimport base64\nimport io\n\n\nclass StableDiffusion(Component):\n    display_name = \"StableDiffusion\"\n    description = \"StableDiffusion Image Genearator\"\n    icon = \"image\"\n    name = \"StableDiffusion\"\n\n    inputs = [\n        MessageTextInput(\n            name=\"prompt\",\n            display_name=\"Prompt\",\n            info=\"Text input to generate image\",\n            value=\"Astronaut riding a horse\",\n        ),\n        DropdownInput(\n            name=\"model\",\n            display_name=\"Model\",\n            info=\"Diffrent models available\",\n            options=[\n                \"stable-diffusion-3.5-large\",\n                \"stable-diffusion-3.5-large-turbo\",\n                \"stable-diffusion-3.5-medium\",\n                \"stable-diffusion-3-medium\",\n            ],\n            value=\"stable-diffusion-3.5-large\",\n        ),\n        SecretStrInput(\n            name=\"api_token\",\n            display_name=\"HuggingFace API Token\",\n            info=\"HuggingFace Access Token\",\n            password=True,\n        ),\n        MessageTextInput(\n            name=\"base_url\",\n            display_name=\"Base URL\",\n            info=\"Base enpoint URL\",\n            value=\"https://api-inference.huggingface.co/models/stabilityai\",\n            advanced=True,\n        ),\n    ]\n\n    outputs = [\n        Output(\n            display_name=\"Base64 Image\", name=\"base64_image\", method=\"generate_image\"\n        ),\n    ]\n\n    def generate_image(self) -> Message:\n        response = self.query()\n        image_base64 = base64.b64encode(io.BytesIO(response.content).getvalue())\n        return Message(text=image_base64)\n\n    def query(self):\n        prompt = self.prompt\n        model = self.model\n        api_token = self.api_token\n        base_url = self.base_url\n\n        API_URL = f\"{base_url}/{model}\"\n        headers = {\"Authorization\": f\"Bearer {api_token}\"}\n\n        payload = {\"inputs\": prompt}\n        response = requests.post(API_URL, headers=headers, json=payload)\n        return response\n",
